[<%_ body.forEach(function (item, i, arr) { _%>
  {
    "config": {
    <%_ for(key in item['config']){ _%>
      "<%=key %>": {
      <%_ if (typeof item['config'][key] === 'object') { _%>
        <%_ for(anotherKey in item['config'][key]){ _%>
          <% if(typeof item['config'][key][anotherKey] === 'object'){ %>
            <% for(yetAnotherKey in item['config'][key][anotherKey]){ %>        
              {<% if (typeof item['config'][key][anotherKey][yetAnotherKey] === 'object' ) { %>
                <% for (lastKey in item['config'][key][anotherKey][yetAnotherKey]){ %>
                  "<%=lastKey%>":"<%=item['config'][key][anotherKey][yetAnotherKey][lastKey]%>"
                <%}%>
              <%}else {%>
                "<%=yetAnotherKey%>":"<%=item['config'][key][anotherKey][yetAnotherKey] %>"
              <%}%>}
            <%}%>
          <%}else{ %>
            "<%=anotherKey%>": "<%=item['config'][key][anotherKey]%>",
          <% }%>
        <%}%>
      <%} else{%>
        "<%=key%>":"<%=item['config'][key]%>"
      <% }%>
      }
    <%}%>
    }
  <%= ( arr.length > 0 && i < arr.length-1 ) ? '},': '' %>
  <% }); %>
]
